<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<Appenders>
<Console name="std_out" target="SYSTEM_OUT" follow="true">

  <PatternLayout pattern="%d{MM-dd-yyyy HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
</Console>
</Appenders>
<Loggers>
<Root level="ERROR">
  <AppenderRef ref="std_out"/>
</Root>
</Loggers>
</Configuration>